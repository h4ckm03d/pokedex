{
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "overrides": [
      {
        "files": ["templates/winter/*.js"],
        "env": {
          "browser": true
        },
        "parserOptions": {
          "sourceType": "module"
        }
      }
    ]
  }